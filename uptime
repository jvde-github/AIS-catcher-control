#!/bin/bash

# Define the PID file path
pidfile="/etc/AIS-catcher/aiscatcher.pid"

# Check if the PID file exists
if [[ ! -f "$pidfile" ]]; then
  echo "Not running"
  exit 1
fi

# Read the PID from the file
pid=$(cat "$pidfile")

# Check if the process with the PID exists
if [[ ! -d "/proc/$pid" ]]; then
  echo "Not running"
  exit 1
fi

# Get the start time of the process
start_time=$(ps -p "$pid" -o lstart=)

# Output the start time
echo "$start_time"
